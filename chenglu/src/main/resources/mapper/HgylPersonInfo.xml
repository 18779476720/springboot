    <select id="list" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from `hgyl_person_info`
        <trim prefix="where " prefixOverrides="and ">
            <if test="id != null">
                and `id` = #{id}
            </if>
            <if test="area != null">
                and `area` = #{area}
            </if>
            <if test="jobNum != null">
                and `job_num` = #{jobNum}
            </if>
            <if test="name != null">
