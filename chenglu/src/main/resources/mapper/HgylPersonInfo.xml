    <!--批量插入-->
    <insert id="batchInsert" parameterType="java.util.List">
        INSERT INTO
        hgyl_person_info(
        `area`,
        `job_num`,
        `name`,
        `project`,
        `working`,
        `site`,
        `start_date`,
        `end_date`,
        `remark`)
        VALUES
        <foreach collection="list" item="item" index="index"
                 separator=",">
            (
            #{item.area},
            #{item.jobNum},
            #{item.name},
            #{item.project},
            #{item.working},
            #{item.site},
            #{item.startDate},
            #{item.endDate},
            #{item.remark}
            )
        </foreach>
